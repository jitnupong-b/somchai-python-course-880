{
    "editor.fontSize": 20,
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "Cascadia Mono",
    "editor.suggest.showVariables": true,
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
